<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>SwapFlex Server</title>
    <link rel="stylesheet" href="/css/login.css" />
  </head>
  <body>
    <button onclick="window.location.href='/app/login'">
      Login to SwapFlex Server
    </button>
  </body>
</html>


<ul class="items">
          <% for (const rateObject of rates) { %> <% for (const [currency,
          value] of Object.entries(rateObject)) { %>
          <li><%= currency %>: ₦<%= value %></li>
          <% } %> <% } %>
        </ul>

        <h3>Max Amounts</h3>

        <ul class="items">
          <% for (const rateObject of max) { %> <% for (const [currency, value]
          of Object.entries(rateObject)) { %>
          <li><%= currency %>: ₦<%= value %></li>
          <% } %> <% } %>
        </ul>

        <button onclick="window.location.href='/app/update-rates'">
          Update Rates
        </button>
      </div>

      <div class="box">
        <h3>Account Details</h3>

        <ul class="items">
          <h4>Dollar Account</h4>
          <% for (const [key, value] of Object.entries(account_usd)) { %>
          <li><strong><%= key %>:</strong> <%= value %></li>
          <% } %>
        </ul>
        <button onclick="window.location.href='/app/update-accounts/usd'">
          Update Dollar Account
        </button>

        <ul class="items">
          <h4>Euros Account</h4>
          <% for (const [key, value] of Object.entries(account_eur)) { %>
          <li><strong><%= key %>:</strong> <%= value %></li>
          <% } %>
        </ul>